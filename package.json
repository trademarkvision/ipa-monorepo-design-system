{
  "name": "@tmv/ipa-design-system",
  "version": "0.1.19",
  "private": true,
  "scripts": {
    "build": "node script-build-local",
    "prebuild": "npm install",
    "build:ci": "node script-build-local",
    "build:npm": "node script-build-npm",
    "prebuild:npm": "npm install",
    "build:npm:report": "vue-cli-service build --target lib --formats commonjs --name ipa-design-system ./src/library.ts --mode report",
    "prebuild:npm:report": "npm install",
    "send": "npm install && npm run build:npm && ./script-publish.sh",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit --watch",
    "storybook": "vue-cli-service storybook:serve -p 6006 -c config/storybook",
    "storybook:build": "vue-cli-service storybook:build -c config/storybook",
    "watch": "vue-cli-service build --watch --target lib --formats commonjs --name ipa-design-system ./src/library.ts"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.31",
    "@fortawesome/free-regular-svg-icons": "5.15.0",
    "@fortawesome/free-solid-svg-icons": "5.15.0",
    "@fortawesome/vue-fontawesome": "2.0.0",
    "axios": "0.21.1",
    "core-js": "3.7.0",
    "lodash.debounce": "4.0.8",
    "lodash.merge": "4.6.2",
    "qs": "6.9.4",
    "scroll-lock": "2.1.4",
    "vue": "2.6.12",
    "vue-class-component": "7.2.6",
    "vue-multiselect-ipa": "2.1.0",
    "vue-property-decorator": "9.0.2",
    "vue-wysiwyg-2": "1.7.94",
    "vuex": "3.5.1",
    "vuex-simple": "2.2.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "5.2.6",
    "@storybook/addon-actions": "5.2.6",
    "@storybook/addon-knobs": "5.2.6",
    "@storybook/addon-links": "5.2.6",
    "@storybook/addon-notes": "5.2.6",
    "@storybook/addon-storyshots": "5.2.6",
    "@storybook/addon-viewport": "5.2.6",
    "@types/jest": "23.3.14",
    "@types/lodash.debounce": "4.0.5",
    "@types/lodash.merge": "4.6.6",
    "@types/qs": "6.5.1",
    "@vue/cli-plugin-babel": "3.6.0",
    "@vue/cli-plugin-eslint": "3.6.0",
    "@vue/cli-plugin-typescript": "3.6.0",
    "@vue/cli-plugin-unit-jest": "3.6.3",
    "@vue/cli-service": "3.6.0",
    "@vue/eslint-config-typescript": "4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "axios": "0.21.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-minify-webpack-plugin": "0.3.1",
    "babel-plugin-require-context-hook": "1.0.0",
    "chalk": "2.4.2",
    "diff": "4.0.1",
    "eslint": "5.16.0",
    "eslint-plugin-vue": "5.2.2",
    "fs-extra": "7.0.1",
    "glob": "7.1.3",
    "gulp-insert": "0.5.0",
    "gulp-sort": "2.0.0",
    "jest-vue-preprocessor": "1.5.0",
    "lodash.debounce": "4.0.8",
    "object-hash": "1.3.1",
    "proper-lockfile": "4.1.1",
    "sass": "1.19.0",
    "sass-loader": "7.1.0",
    "semver": "6.2.0",
    "sort-stream": "1.0.1",
    "stats-webpack-plugin": "0.7.0",
    "ts-jest": "23.10.5",
    "typescript": "3.4.4",
    "vinyl-fs": "3.0.3",
    "vinyl-fs-concat": "0.0.1",
    "vinyl-map": "1.0.2",
    "vue-cli-plugin-storybook": "0.6.1",
    "vue-multiselect-ipa": "2.1.0",
    "vue-template-compiler": "2.6.10",
    "vue-wysiwyg-2": "1.7.94",
    "webpack-bundle-analyzer": "3.0.4",
    "yargs": "13.2.2"
  },
  "targets": {
    "library-local": {
      "module": "./src/library.ts",
      "types": "./src/library.ts",
      "files": [
        "src/*",
        "fonts/*",
        "images/**/*",
        "public/*",
        "internal/*",
        "styles/*"
      ],
      "sideEffects": [
        "src/globals/*.ts"
      ]
    },
    "library-npm": {
      "main": "./dist/ipa-design-system.common.js",
      "types": "./dist/ipa-design-system.common.d.ts",
      "files": [
        "dist/*",
        "fonts/*",
        "images/**/*",
        "public/*",
        "internal/*",
        "styles/*",
        "build.hash"
      ]
    }
  }
}
